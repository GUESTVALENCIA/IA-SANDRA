    "start:sonnet-mcp": "node mcp-server/mcp-sonnet-full-access.js",
    "build:win": "electron-builder --config electron-builder.yml --win --x64",
    "build:mac": "electron-builder --config electron-builder.yml --mac",
    "build": "electron-builder --config electron-builder.yml",
    "call-router": "node services/call-router/index.js",
    "avatar-streamer": "node services/avatar-streamer/index.js",
    "metrics": "node services/metrics-exporter/index.js",
    "log-ingestor": "node services/log-ingestor/index.js",
    "cost-alert": "node services/cost-alert/index.js",
    "dev:pwa": "npm --prefix guests-pwa run dev",
    "build:pwa": "npm --prefix guests-pwa run build",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@deepgram/sdk": "^4.11.2",
    "@octokit/rest": "^22.0.1",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "electron": "^39.1.2",
    "electron-updater": "^6.6.2",
    "express": "^4.21.2",
    "express-ws": "^5.0.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "g711": "^1.0.1",
    "glob": "^13.0.0",
    "node-cron": "^4.2.1",
    "node-webrtc": "^0.0.0",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "puppeteer-core": "^24.30.0",
    "semver": "^7.7.3",
    "twilio": "^5.10.5",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "electron-builder": "^26.0.12",
    "jest": "^30.2.0",
    "node-fetch": "^2.7.0",
    "vercel": "^48.10.4"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
