{
  "name": "sandra-ia-mobile-galaxy",
  "version": "98.0.0",
  "description": "Sandra IA Mobile Galaxy Ultimate - Galaxy Level PWA optimizada para iOS 14+ con soporte completo para iPhone Pro y iPad Pro",
  "main": "sandra-mcp-bridge.js",
  "homepage": "https://sandra.guestsvalencia.es",
  "scripts": {
    "start": "node backend/server.js",
    "start:original": "node sandra-mcp-bridge.js",
    "dev": "nodemon sandra-mcp-bridge.js",
    "build": "npm run build:production",
    "build:production": "npm run optimize:assets && npm run generate:manifest && npm run build:sw",
    "build:staging": "NODE_ENV=staging npm run build:production",
    "build:development": "NODE_ENV=development npm run build:production",
    "optimize:assets": "npm run optimize:html && npm run optimize:js && npm run optimize:css",
    "optimize:html": "echo 'Optimizing HTML files...'",
    "optimize:js": "echo 'Optimizing JavaScript files...'",
    "optimize:css": "echo 'Optimizing CSS files...'",
    "generate:manifest": "node scripts/generate-manifest.js",
    "build:sw": "node scripts/build-service-worker.js",
    "deploy": "npm run build:production && netlify deploy --prod",
    "deploy:staging": "npm run build:staging && netlify deploy",
    "deploy:preview": "npm run build:development && netlify deploy",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --testMatch='**/tests/unit/**/*.test.js'",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.js'",
    "test:e2e": "playwright test tests/e2e/sandra-chat-flow.spec.js",
    "test:e2e:all": "playwright test tests/e2e/ tests/",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "playwright test tests/performance/",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:performance",
    "test:quick": "jest --testMatch='**/tests/unit/**/*.test.js' --bail",
    "test:accessibility": "pa11y https://sandra.guestsvalencia.es",
    "test:security": "echo 'Running security tests...'",
    "lint": "eslint *.js",
    "lint:fix": "eslint *.js --fix",
    "format": "prettier --write *.js *.html *.css *.json",
    "analyze": "npm run analyze:bundle && npm run analyze:performance",
    "analyze:bundle": "echo 'Analyzing bundle size...'",
    "analyze:performance": "echo 'Analyzing performance...'",
    "serve": "http-server . -p 8080 -c-1",
    "serve:https": "http-server . -p 8443 -S -C cert.pem -K key.pem",
    "mobile:ios": "node scripts/mobile/ios-testflight-setup.js",
    "mobile:android": "node scripts/mobile/generate-apk.js",
    "mobile:build": "npm run mobile:ios && npm run mobile:android",
    "build:downloads": "node scripts/build-downloads.js",
    "test:downloads": "node scripts/test-downloads.js",
    "docs": "echo 'Generating documentation...'",
    "docs:serve": "echo 'Serving documentation...'",
    "clean": "rm -rf dist/ build/ reports/ .cache/",
    "clean:cache": "rm -rf .cache/ node_modules/.cache/",
    "precommit": "npm run lint && npm run test:unit",
    "prepush": "npm run smoke",
    "smoke": "node scripts/smoke.js",
    "postinstall": "npm run build:sw",
    "rotation:start": "node scripts/security/key-rotation.js rotate",
    "rotation:verify": "node scripts/security/key-rotation.js verify",
    "rotation:verify-weekly": "node scripts/security/key-rotation.js verify",
    "rotation:emergency-revoke": "node scripts/security/key-rotation.js emergency",
    "rotation:provider": "node scripts/security/key-rotation.js rotate-provider",
    "secrets:scan": "node scripts/security/secret-scanner.js",
    "secrets:scan-ci": "node scripts/security/secret-scanner.js --ci",
    "security:monitor": "node scripts/key-expiration-monitor.js",
    "security:dashboard": "http-server public/admin -p 8081",
    "security:audit": "node scripts/security/security-audit.js",
    "security:setup": "node scripts/security/setup-automation.js",
    "test:api-keys": "node scripts/security/test-api-keys.js",
    "deploy:update-env": "node scripts/security/update-netlify-env.js",
    "autorun:setup": "node scripts/aws-secrets-setup.js",
    "autorun:start": "node scripts/key-rotation-autorun.js",
    "autorun:pm2": "pm2 start scripts/key-rotation-autorun.js --name sandra-rotation && pm2 save",
    "autorun:status": "pm2 status sandra-rotation",
    "autorun:logs": "pm2 logs sandra-rotation",
    "autorun:restart": "pm2 restart sandra-rotation",
    "autorun:stop": "pm2 stop sandra-rotation",
    "autorun:report": "node -e \"const {SandraKeyRotator}=require('./scripts/key-rotation-autorun.js');const r=new SandraKeyRotator();r.initialize().then(()=>r.sendWeeklyReport());\"",
    "autorun:deepgram": "node -e \"const {SandraKeyRotator}=require('./scripts/key-rotation-autorun.js');const r=new SandraKeyRotator();r.initialize().then(()=>r.rotateDeepgramKey());\"",
    "autorun:verify": "node scripts/verify-autorun-setup.js",
    "backend:setup": "node scripts/setup-backend.js",
    "backend:test": "node tests/backend/api-tests.js",
    "backend:dev": "netlify dev",
    "backend:health": "curl http://localhost:8888/.netlify/functions/health",
    "backend:metrics": "curl -X POST http://localhost:8888/.netlify/functions/metrics -H 'Content-Type: application/json' -d '{\"action\":\"stats\"}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ClayTom/sandra-ia-mobile.git"
  },
  "keywords": [
    "sandra",
    "ia",
    "mobile",
    "pwa",
    "galaxy",
    "ai",
    "voice",
    "chat",
    "ios",
    "android",
    "responsive",
    "guestsvalencia"
  ],
  "author": "Sandra IA Team - GuestsValencia",
  "license": "PROPRIETARY",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@deepgram/sdk": "^4.11.2",
    "@heygen/streaming-avatar": "^2.1.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "aws-sdk": "^2.1542.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "electron": "^39.0.0",
    "express": "^4.21.1",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "jszip": "^3.10.1",
    "livekit-client": "^2.15.14",
    "marked": "^16.4.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "openai": "^4.58.1",
    "pdf-parse": "^2.4.5",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "redis": "^5.9.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "socket.io-redis": "^6.1.1",
    "twilio": "^5.10.4",
    "uuid": "^13.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@playwright/test": "^1.56.1",
    "babel-jest": "^30.2.0",
    "clean-css": "^5.3.3",
    "electron": "^27.0.0",
    "eslint": "^8.55.0",
    "html-minifier": "^4.0.0",
    "http-server": "^14.1.1",
    "jest": "^30.2.0",
    "lighthouse": "^11.4.0",
    "netlify-cli": "^17.38.1",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.2",
    "pa11y": "^6.2.3",
    "prettier": "^3.1.0",
    "terser": "^5.26.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "iOS >= 14",
    "Android >= 8",
    "Chrome >= 88",
    "Firefox >= 85",
    "Safari >= 14",
    "Edge >= 88"
  ],
  "pwa": {
    "enabled": true,
    "scope": "/",
    "startUrl": "/",
    "display": "standalone",
    "orientation": "portrait-primary",
    "themeColor": "#00ff88",
    "backgroundColor": "#0a0a0a"
  },
  "mobile": {
    "ios": {
      "enabled": true,
      "minimumVersion": "14.0"
    },
    "android": {
      "enabled": true,
      "minimumVersion": "8.0"
    }
  },
  "performance": {
    "budgets": {
      "bundle": "250kb",
      "initial": "100kb",
      "assets": "500kb"
    },
    "targets": {
      "lighthouse": 90,
      "accessibility": 95,
      "performance": 90,
      "seo": 100
    }
  },
  "security": {
    "csp": true,
    "https": true,
    "headers": true
  },
  "deployment": {
    "platform": "netlify",
    "domain": "sandra.guestsvalencia.es",
    "ssl": true,
    "cdn": true,
    "compression": true
  }
}
