  "protocol_handlers": [
    {
      "protocol": "web+sandra",
      "url": "/?action=%s"
    },
    {
      "protocol": "mailto",
      "url": "/?action=compose&to=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/?action=open",
      "accept": {
        "text/plain": [
          ".txt"
        ],
        "text/markdown": [
          ".md"
        ],
        "application/pdf": [
          ".pdf"
        ],
        "image/*": [
          ".jpg",
          ".jpeg",
          ".png",
          ".gif",
          ".webp",
          ".svg"
        ],
        "audio/*": [
          ".mp3",
          ".wav",
          ".m4a",
          ".ogg"
        ],
        "video/*": [
          ".mp4",
          ".mov",
          ".avi",
          ".webm"
        ]
      }
    }
  ],
  "share_target": {
    "action": "/?action=share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": [
            "image/*",
            "audio/*",
            "video/*",
            "text/*",
            "application/pdf"
          ]
        }
      ]
    }
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
